pages = {E763--E778},
pmid = {1616014},
title = {{Interactions between ANG II, sympathetic nervous system, and baroreceptor reflexes in regulation of blood pressure}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/1616014 http://www.physiology.org/doi/10.1152/ajpendo.1992.262.6.E763},
