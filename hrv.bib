@misc{Vaccarino2013a,
abstract = {Comments on an article by J. H. Turner et al. (see record 2013-30668-001). There is growing recognition that the consequences of posttraumatic stress disorder (PTSD) on health may reach far beyond the neuropsychiatric sphere. For many years, studies have documented numerous physical health problems in PTSD, and a possible connection between PTSD and risk of coronary heart disease (CHD) has received special attention. The study by Turner et al. confirms these previous findings in a sample of patients from outpatient clinics of two Veteran Affairs Medical Centers. In this study, ischemia was assessed by exercise electrocardiography (ECG). Exercise ECG testing is considered less accurate for the assessment of ischemia than stress testing in conjunction with myocardial perfusion imaging, especially because of low specificity and because ST-segment changes during treadmill ECG testing are weak markers of prevalent or incident ischemic heart disease. Despite these limitations, the results are impressively similar to the twin study mentioned earlier, showing about twice the prevalence of ischemia in patients with PTSD compared with those without the disease. Non-ECG measures of exercise treadmill testing-in particular, exercise capacity-have emerged as stronger predictors of cardiovascular risk than ECG measures. It is unfortunate that exercise capacity data from the treadmill tests were not presented in this article because they would have been useful complementary information. The study by Turner et al. provides welcome new evidence of a link between PTSD and CHD. The mechanisms behind this relationship, however, still need to be understood. Clearly, maladaptive behaviors, such as smoking and substance abuse, which are almost invariably more common in persons with PTSD, may be implicated. Lower propensity to seek medical care and poor functioning could translate to reduced self-care or reduced access to health care. In addition to behavioral and lifestyle factors, however, it has long been suggested that neurobiological features characteristic of PTSD could have potential damaging effects on the cardiovascular system. (PsycINFO Database Record (c) 2014 APA, all rights reserved)},
archivePrefix = {arXiv},
arxivId = {15334406},
author = {Vaccarino, Viola and Bremner, J. Douglas},
booktitle = {Biological Psychiatry},
doi = {10.1016/j.biopsych.2013.10.002},
eprint = {15334406},
isbn = {0006-3223},
issn = {00063223},
month = {dec},
number = {11},
pages = {790--792},
pmid = {24188697},
title = {{Traumatic stress is heartbreaking}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24188697 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4667744 https://linkinghub.elsevier.com/retrieve/pii/S0006322313009062},
volume = {74},
