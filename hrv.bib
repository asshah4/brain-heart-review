pmid = {29735423},
title = {{Association of positive well-being with reduced cardiac repolarization abnormalities in the First National Health and Nutrition Examination Survey}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/29735423 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5994381 https://linkinghub.elsevier.com/retrieve/pii/S0167527317323847},
