journal = {Canadian Journal of Physiology and Pharmacology},
keywords = {Animals,Dogs,Electric Stimulation,Evoked Potentials,Female,Heart Conduction System,Heart Conduction System: physiopathology,Male,Myocardial Infarction,Myocardial Infarction: physiopathology,done},
