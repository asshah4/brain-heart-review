issn = {0363-6119},
journal = {American Journal of Physiology-Regulatory, Integrative and Comparative Physiology},
keywords = {cardiovascular disease,posttraumatic stress disorder,renin-angiotensin system},
month = {aug},
number = {4},
pages = {R315--R321},
pmid = {26062635},
publisher = {American Physiological Society Bethesda, MD},
title = {{Autonomic and inflammatory consequences of posttraumatic stress disorder and the link to cardiovascular disease}},
url = {http://ajpregu.physiology.org/lookup/doi/10.1152/ajpregu.00343.2014 http://www.physiology.org/doi/10.1152/ajpregu.00343.2014},
