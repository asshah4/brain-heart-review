pmid = {27719650},
title = {{The Insular Cortex and Takotsubo Cardiomyopathy}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/27719650 http://www.eurekaselect.com/openurl/content.php?genre=article&issn=1381-6128&volume=23&issue=6&spage=879},
