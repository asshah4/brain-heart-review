pmid = {29543649},
title = {{Heart and soul: Heart rate variability and major depression}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/29543649 http://insights.ovid.com/crossref?an=00008877-201804000-00006},
volume = {29},
