journal = {Biomedicine & pharmacotherapy = Biom{\'{e}}decine & pharmacoth{\'{e}}rapie},
keywords = {Aged,Ambulatory,Coronary Artery Bypass,Coronary Artery Bypass: adverse effects,Death,Electrocardiography,Entropy,Female,Heart Rate,Heart Rate: physiology,Humans,Male,Middle Aged,Predictive Value of Tests,Prognosis,Sudden},
month = {nov},
pages = {110s--114s},
pmid = {11774857},
publisher = {Elsevier Masson},
title = {{Changes in ultra-low and very low frequency heart rate variability after coronary artery bypass grafting.}},
