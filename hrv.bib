doi = {10.1016/j.jacc.2005.03.043},
file = {:Users/shahbook/Box Sync/Mendeley_Articles/2005 - Zarse et al. - Selective increase of cardiac neuronal sympathetic tone A catheter-based access to modulate left ventricular contr.pdf:pdf},
isbn = {1558-3597 (Electronic)\r0735-1097 (Linking)},
issn = {07351097},
