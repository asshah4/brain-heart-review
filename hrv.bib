title = {{Sex differences in mental stress-induced myocardial ischemia in young survivors of an acute myocardial infarction}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24608039 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4008686 https://insights.ovid.com/crossref?an=00006842-201404000-00007},
volume = {76},
