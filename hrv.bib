pmid = {26352544},
title = {{Heart rate variability in the prediction of risk for posttraumatic stress disorder}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/26352544 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5872816 http://archpsyc.jamanetwork.com/article.aspx?doi=10.1001/jamapsychiatry.2015.1394},
