month = {apr},
number = {3},
pages = {162--169},
pmid = {25739828},
publisher = {John Wiley & Sons, Ltd},
title = {{Cadherin-13 gene is associated with hyperactive/impulsive symptoms in attention/deficit hyperactivity disorder}},
url = {http://doi.wiley.com/10.1002/ajmg.b.32293},
volume = {168},
year = {2015}
