@article{Moak2007,
abstract = {BACKGROUND: Power spectral analysis of heart rate variability (HRV) has been used to indicate cardiac autonomic function. High-frequency power relates to respiratory sinus arrhythmia and therefore to parasympathetic cardiovagal tone; however, the relationship of low-frequency (LF) power to cardiac sympathetic innervation and function has been controversial. Alternatively, LF power might reflect baroreflexive modulation of autonomic outflows.\n\nOBJECTIVE: We studied normal volunteers and chronic autonomic failure syndrome patients with and without loss of cardiac noradrenergic nerves to examine the relationships of LF power with cardiac sympathetic innervation and baroreflex function.\n\nMETHODS: We compared LF power of HRV in patients with cardiac sympathetic denervation, as indicated by low myocardial concentrations of 6-[(18)F] fluorodopamine-derived radioactivity or low rates of norepinephrine entry into coronary sinus plasma (cardiac norepinephrine spillover) to values in patients with intact innervation, at baseline, during infusion of yohimbine, which increases exocytotic norepinephrine release from sympathetic nerves, or during infusion of tyramine, which increases non-exocytotic release. Baroreflex-cardiovagal slope (BRS) was calculated from the cardiac interbeat interval and systolic pressure during the Valsalva maneuver.\n\nRESULTS: LF power was unrelated to myocardial 6-[(18)F] fluorodopamine-derived radioactivity or cardiac norepinephrine spillover. In contrast, the log of LF power correlated positively with the log of BRS (r=0.72, P <0.0001). Patients with a low BRS (<or=3 msec/mm Hg) had low LF power, regardless of cardiac innervation. Tyramine and yohimbine increased LF power in subjects with normal BRS but not in those with low BRS. BRS at baseline predicted LF responses to tyramine and yohimbine.\n\nCONCLUSION: LF power reflects baroreflex function, not cardiac sympathetic innervation.},
archivePrefix = {arXiv},
arxivId = {NIHMS150003},
author = {Moak, Jeffrey P and Goldstein, David S and Eldadah, Basil A and Saleem, Ahmed and Holmes, Courtney and Pechnik, Sandra and Sharabi, Yehonatan},
doi = {10.1016/j.hrthm.2007.07.019},
eprint = {NIHMS150003},
file = {:Users/shahbook/Box Sync/Mendeley_Articles/2007 - Moak et al. - Supine low-frequency power of heart rate variability reflects baroreflex function, not cardiac sympathetic innervat.pdf:pdf},
isbn = {1939-2869 (Electronic)\r0891-1150 (Linking)},
issn = {1547-5271},
journal = {Heart rhythm : the official journal of the Heart Rhythm Society},
keywords = {Adrenergic Fibers,Adrenergic Fibers: physiology,Adrenergic alpha-Antagonists,Adrenergic alpha-Antagonists: pharmacology,Autonomic Nervous System Diseases,Baroreflex,Baroreflex: physiology,Heart,Heart Rate,Heart Rate: drug effects,Heart Rate: physiology,Heart: innervation,Heart: physiology,Humans,Hypotension,Orthostatic,Supine Position,Sympathomimetics,Sympathomimetics: pharmacology,Tyramine,Tyramine: pharmacology,Yohimbine,Yohimbine: pharmacology},
number = {12},
pages = {1523--9},
pmid = {17997358},
title = {{Supine low-frequency power of heart rate variability reflects baroreflex function, not cardiac sympathetic innervation.}},
url = {https://www-ncbi-nlm-nih-gov.proxy.library.emory.edu/pmc/articles/PMC2204059/pdf/nihms37087.pdf http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2204059&tool=pmcentrez&rendertype=abstract},
