isbn = {0305-1870 (Print)},
issn = {0305-1870},
journal = {Clin Exp Pharmacol Physiol},
keywords = {Adenosine/*pharmacology,Afferent/*physiology,Animals,Cardiovascular Agents/*pharmacology,Heart Conduction System/physiopathology,Humans,Myocardial Ischemia/*physiopathology,Neurons,Reflex/*physiology,Sympathetic Nervous System/physiopathology,Vagus Nerve/physiology,done},
mendeley-tags = {done},
