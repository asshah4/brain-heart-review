pmid = {26111942},
title = {{Physical Activity, Mindfulness Meditation, or Heart Rate Variability Biofeedback for Stress Reduction: A Randomized Controlled Trial}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/26111942 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4648965 http://link.springer.com/10.1007/s10484-015-9293-x},
