keywords = {Analysis of Variance,Animals,Arrhythmia,Atropine,Atropine: pharmacology,Cats,Cerebral Decortication,Heart Rate,Propranolol,Propranolol: pharmacology,Respiration,Sinus,Sinus: etiology,Spectrum Analysis,Sympathetic Nervous System,Sympathetic Nervous System: physiology,Vagotomy,Vagus Nerve,Vagus Nerve: physiology},
number = {3},
pages = {775--80},
pmid = {1115243},
title = {{Influence of cardiac neural inputs on rhythmic variations of heart period in the cat.}},
