file = {:Users/shahbook/Box Sync/Mendeley_Articles/1977 - Schwartz, Verrier, Lown - Effect of stellectomy and vagotomy on ventricular refractoriness in dogs.pdf:pdf},
issn = {0009-7330},
journal = {Circulation Research},
month = {jun},
